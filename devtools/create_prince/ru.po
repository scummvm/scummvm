msgid "Silmaniona - "
msgstr "Силманион - "

#: mob.lst:50009
msgid "Kurhan - "
msgstr "курган - "

#: mob.lst:50010
msgid "Smocza jaskinia - "
msgstr "пещера дракона - "

#: mob.lst:50011
msgid "Młyn - "
msgstr "мельница - "

